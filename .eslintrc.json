{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended",
        "prettier",
        "plugin:security/recommended"
    ],
    "env": {
        "es2021": true,
        "node": true
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": [
            "error",
            { "vars": "all", "args": "after-used", "ignoreRestSiblings": true }
        ]
    }
}
